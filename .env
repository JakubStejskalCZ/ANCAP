# Discord bot token (required)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Database URL (default: SQLite in the project directory)
DATABASE_URL=sqlite+aiosqlite:///database.db

# Command prefix for the bot (default: "!")
PREFIX=!

# Default locale for translations (e.g., "en" for English, "cs" for Czech)
LOCALE=en

# Guild name where the bot operates (required for tasks)
GUILD_NAME=YourGuildNameHere

# Test mode (default: False, set to True for testing without background tasks)
TEST_MODE=False
